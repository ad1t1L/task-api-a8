{
  "document": {
    "o": { "j": 1, "k": 2 },
    "a": [5, 3]
  },
  "queries": [
    {
      "path": "$[*]",
      "values": [{ "j": 1, "k": 2 }, [5, 3]],
      "paths": ["$['o']", "$['a']"]
    },
    {
      "path": "$.o[*]",
      "values": [1, 2],
      "paths": ["$['o']['j']", "$['o']['k']"]
    },
    {
      "path": "$.o[*, *]",
      "values": [1, 2, 1, 2],
      "paths": ["$['o']['j']", "$['o']['k']", "$['o']['j']", "$['o']['k']"]
    },
    {
      "path": "$.a[*]",
      "values": [5, 3],
      "paths": ["$['a'][0]", "$['a'][1]"]
    }
  ]
}
